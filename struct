

typedef struct sym_node{
                       int name[32];
                       int type;
                       int address;
                        *sym_node next;}

typedef struct data_node{
                         int data:12 ;
                          int address;
                           *dat_node next }


typedef struct com_node{
                        int op1:3;
                        int optype:4;
                        int op2:3;
                        int ARE:2;
                        int word:12;
                        int half1:6;
                        int half2:6;
                          *com_node next;}
                                       
